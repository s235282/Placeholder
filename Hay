print(A)
yo = nx.from_numpy_array(A)
pos = nx.circular_layout(yo)
plt.figure(figsize=(8, 6), facecolor='black')
ax = plt.gca()
ax.set_facecolor('black')
nx.draw_networkx_nodes(yo, pos, node_color='white',
                       edgecolors='black', node_size=100)

nx.draw_networkx_edges(yo, pos, edgelist=yo.edges(), edge_color='blue')

missing_edges = [(i, j) for i in yo.nodes()
                 for j in yo.nodes() if i < j and not yo.has_edge(i, j)]

nx.draw_networkx_edges(yo, pos, edgelist=missing_edges, edge_color='red')

plt.axis('off')


#nx.draw(yo, pos, with_labels=True, node_color='lightblue', edge_color='gray',
#        node_size=700, font_size=16, font_weight='bold')


plt.title("Graph Visualization")
plt.show()
